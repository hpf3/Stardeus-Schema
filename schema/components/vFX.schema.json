{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "Component": {
            "const": "VFX"
        },
        "Properties": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "Key": {
                        "enum": [
                            "IsCosmetic",
                            "IsLinkedToEnergy",
                            "SkipOnOSX",
                            "Id",
                            "Id_U",
                            "Id_D",
                            "Id_L",
                            "Id_R",
                            "Offsets",
                            "Offset"
                        ]
                    }
                },
                "allOf": [
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "enum": [
                                        "IsCosmetic",
                                        "IsLinkedToEnergy",
                                        "SkipOnOSX"
                                    ]
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/Bool"
                        }
                    },
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "enum": [
                                        "Id",
                                        "Id_U",
                                        "Id_D",
                                        "Id_L",
                                        "Id_R"
                                    ]
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/String"
                        }
                    },
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "const": "Offsets"
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/Vector2Set"
                        }
                    },
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "const": "Offset"
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/Vector2"
                        }
                    }
                ]
            }
        }
    },
    "required": [
        "Component",
        "Properties"
    ]
}