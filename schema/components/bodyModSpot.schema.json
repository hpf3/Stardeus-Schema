{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "Component": {
            "const": "BodyModSpot"
        },
        "Properties": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "Key": {
                        "enum": [
                            "EnergyCost",
                            "DefaultPriority",
                            "Type",
                            "SlotUseWorkSpot",
                            "SpotRotation",
                            "SlotOffsets_L",
                            "SlotOffsets_R",
                            "SlotOffsets_U",
                            "SlotOffsets_D"
                        ]
                    }
                },
                "allOf": [
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "const": "EnergyCost"
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/Float"
                        }
                    },
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "const": "DefaultPriority"
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/Int"
                        }
                    },
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "enum": ["Type", "SpotRotation"]
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/String"
                        }
                    },
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "const": "SlotUseWorkSpot"
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/Bool"
                        }
                    },
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "enum": [
                                        "SlotOffsets_L",
                                        "SlotOffsets_R",
                                        "SlotOffsets_U",
                                        "SlotOffsets_D"
                                    ]
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/Vector2Set"
                        }
                    }
                ]
            }
        }
    },
    "required": ["Component", "Properties"]
}