{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "Component": {
            "enum": [
                "MatStorage",
                "BeingStorage",
                "ObjStorage"
            ]
        },
        "Properties": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "Key": {
                        "enum": [
                            "Groups",
                            "MaxCapacity",
                            "IsPublic",
                            "ShowFulnessBar",
                            "StorageType"
                        ]
                    }
                },
                "allOf": [
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "const": "Groups"
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/StringSet"
                        }
                    },
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "const": "MaxCapacity"
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/Int"
                        }
                    },
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "const": "IsPublic"
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/Bool"
                        }
                    },
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "const": "ShowFulnessBar"
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/Bool"
                        }
                    },
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "const": "StorageType"
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/String"
                        }
                    }
                ]
            }
        }
    },
    "required": [
        "Component",
        "Properties"
    ]
}