{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "Component": {
            "const": "Planter"
        },
        "Properties": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "Key": {
                        "enum": [
                            "PlantSizes",
                            "SmallPlantCapacity",
                            "MediumPlantCapacity",
                            "LargePlantCapacity",
                            "LargePlantOffsets",
                            "PlantOffsets"
                        ]
                    }
                },
                "allOf": [
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "const": "PlantSizes"
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/StringSet"
                        }
                    },
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "enum":[
                                        "SmallPlantCapacity",
                                        "MediumPlantCapacity",
                                        "LargePlantCapacity"
                                    ]
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/Int"
                        }
                    },
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "enum":[
                                        "LargePlantOffsets",
                                        "PlantOffsets"
                                    ]
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/Vector2Set"
                        }
                    }
                ]
            }
        }
    },
    "required": ["Component", "Properties"]
}