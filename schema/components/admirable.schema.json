{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "Component": {
            "const": "Admirable"
        },
        "Properties": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "Key": {
                        "enum": [
                            "Amount",
                            "Multiplier",
                            "Chance"
                        ]
                    }
                },
                "allOf": [
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "const": "Amount"
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/Int"
                        }
                    },
                    {
                        "if": {
                            "properties": {
                                "Key": {
                                    "enum": [
                                        "Multiplier",
                                        "Chance"
                                    ]
                                }
                            }
                        },
                        "then": {
                            "$ref": "https://raw.githubusercontent.com/hpf3/Stardeus-Schema/stable-web/schema/types.schema.json#/definitions/Float"
                        }
                    }
                ]
            }
        }
    },
    "required": [
        "Component",
        "Properties"
    ]
}